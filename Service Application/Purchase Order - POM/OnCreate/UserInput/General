if(input.General == true)
{
	hide No;
	show Buy_from_Vendor_No;
	show Buy_from_Contact_No;
	show Buy_from_Vendor_Name;
	show Buy_from_Address;
	show Buy_from_Contact;
	show No_of_Achieved_Versions;
	show Posting_Descrption;
	show Electronic_Submission;
	show Posting_Date;
	show Order_Date;
	show Document_Date;
	show Quote_No;
	show Vendor_Order_No;
	show Vendor_Shipment_No;
	show Vendor_Invoice_No;
	show Purchaser_Code;
	show Responsibility_Center;
	show Job_Queue_Status;
	show Status;
	show Processed;
}
else
{
	hide No;
	hide Buy_from_Vendor_No;
	hide Buy_from_Contact_No;
	hide Buy_from_Vendor_Name;
	hide Buy_from_Address;
	hide Buy_from_Contact;
	hide No_of_Achieved_Versions;
	hide Posting_Descrption;
	hide Electronic_Submission;
	hide Posting_Date;
	hide Order_Date;
	hide Document_Date;
	hide Quote_No;
	hide Vendor_Order_No;
	hide Vendor_Shipment_No;
	hide Vendor_Invoice_No;
	hide Purchaser_Code;
	hide Responsibility_Center;
	hide Job_Queue_Status;
	hide Status;
	hide Processed;
}
