/******************
Description: Hide and show based on General
Created AT: 14/12/2022
Created By: Hari Prema M
*****************/
if(input.General == true)
{
	show No;
	show Name;
	show Name_2;
	show Email;
	show Phone_No;
	show Responsibility_Center;
	show Location_of_Service_item;
	show Address;
	show Balance1;
	show Credit_Limit;
	show Sales_Person_Code;
	show Default_Sales_Team;
	show Commisionable;
	show Cust_Emp_Applicable;
	show Cust_Resource_Applicable;
}
else
{
	hide No;
	hide Name;
	hide Name_2;
	hide Email;
	hide Phone_No;
	hide Responsibility_Center;
	hide Location_of_Service_item;
	hide Address;
	hide Balance1;
	hide Credit_Limit;
	hide Sales_Person_Code;
	hide Default_Sales_Team;
	hide Commisionable;
	hide Cust_Emp_Applicable;
	hide Cust_Resource_Applicable;
}
