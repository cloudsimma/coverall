/******************
Description: Hide and show 
Created AT: 14/12/2022
Created By: Hari Prema M
*****************/
if(input.Customer1 == true)
{
	show Customer_No;
	show Address;
	show Name;
	show contact1;
	show Location_of_Service_item;
}
else
{
	hide Customer_No;
	hide Address;
	hide Name;
	hide contact1;
	hide Location_of_Service_item;
}
