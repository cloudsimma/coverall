***********************************************/
if(input.Due_Date_Calculation_in_Days != null)
{
	if(input.Due_Date_Calculation_in_Days < 0)
	{
		alert "This value Shouldn't be less than 0 ";
		input.Due_Date_Calculation_in_Days = null;
	}
}
