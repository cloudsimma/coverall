/*****************************************************
Description: Finishing Date Validation
Created AT : 18/08/2022
Created By : Punitha N
*****************************************************/
if(input.finshing_date != null && input.Response_Date != null)
{
	if(input.finshing_date >= input.Response_Date)
	{
		enable Status;
	}
	else
	{
		disable Status;
		input.finshing_date = null;
		alert "Finshing Date shouldn't be less than Response Date";
	}
}
