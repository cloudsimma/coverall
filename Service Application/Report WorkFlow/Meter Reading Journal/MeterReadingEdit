*****************/
if(input.Status == "Ready to Post")
{
	openUrl("#Form:Meter_Reading_Journals?recLinkID=" + input.ID + "&viewLinkName=Meter_Reading_Journal","popup window");
}
else if(input.Status == "Posted")
{
	openUrl("#Form:Meter_Reading_Journals?recLinkID=" + input.ID + "&viewLinkName=Posted_Meter_Reading_Journals","popup window");
}
