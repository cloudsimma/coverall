/******************
Description: Disable fields for View page option
Created AT: 23/01/2023
Created By: Hari Prema M
*****************/
if(input.Page_Option == "View")
{
	disable Rental_Order_Type;
	disable Date_Time;
	disable Item_Description;
	disable Item_Description1;
	disable Quantity;
	disable Quantity1;
	disable Frequency_Code;
	disable Delivery_Date;
	disable Delivery_Date1;
	disable Unit_Price;
	disable Unit_Price1;
	disable Total_Price;
	disable Total_Price1;
	disable Notes;
	disable Notes1;
}
