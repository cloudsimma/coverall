/**********************************************
Description: Invoice Disc validation 
Created AT : 03/08/2022
Created By : Arun Krishna
***********************************************/
if(input.Invoice_Disc != null)
{
	if(input.Invoice_Disc < 0)
	{
		alert "This value Shouldn't be less than 0 ";
		input.Invoice_Disc = null;
	}
	else if(input.Invoice_Disc > 100)
	{
		alert "The value Shouldn't greater than 100";
		input.Invoice_Disc = null;
	}
}
